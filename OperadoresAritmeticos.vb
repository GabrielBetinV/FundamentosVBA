Public Class OperadoresAritmeticos

 

    Sub Main()
        
        Dim num1 As Integer = 10 'Numero entero
        Dim num2 As Integer = 5 'Numero entero




        Dim suma As Integer = num1 + num2 
        Dim resta As Integer = num1 - num2 
        Dim multiplicacion As Integer = num1 * num2 
        Dim division As As Double = num1 / num2 
        Dim modulo As As Double = num1 Mod num2 
        Dim exponente As Double = num1 ^ num2

    End Sub



End Class



